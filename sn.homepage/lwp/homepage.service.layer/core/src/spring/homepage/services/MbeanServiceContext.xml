<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">

	<bean id="mbeanExporter" class="com.ibm.lconn.homepage.services.mbean.admin.impl.MBeanExporterImpl" init-method="init" destroy-method="destroy">
		<property name="beansToExport">
			<map>
				<entry key="HomepageMetricsService" value-ref="homepageMetricsService" />
				<entry key="HomepagePersonService" value-ref="homepagePersonService" />
				<entry key="HomepageMBeanFacade" value-ref="homepageMBeanFacade" />
			</map>
		</property>
		<property name="wasAdminService" ref="wasAdminService"/>
	</bean>

</beans>
