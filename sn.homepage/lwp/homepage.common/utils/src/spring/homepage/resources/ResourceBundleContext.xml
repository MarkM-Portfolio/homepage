<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">

	<bean id="databaseResourceBundle" class="com.ibm.lconn.homepage.utils.impl.ResourceBundleImpl" scope="singleton">
        <property name="bundleName" value="com.ibm.lconn.homepage.resources.nls.database.DataLogMessages"/>
    </bean>
    
    <bean id="webResourceBundle" class="com.ibm.lconn.homepage.utils.impl.ResourceBundleImpl" scope="singleton">
        <property name="bundleName" value="com.ibm.lconn.homepage.resources.nls.web.WebLogMessages"/>
    </bean>
    
    <bean id="uiResourceBundle" class="com.ibm.lconn.homepage.utils.impl.ResourceBundleImpl" scope="singleton">
        <property name="bundleName" value="com.ibm.lconn.homepage.resources.nls.ui.ui_resources"/>
    </bean>
    
    <bean id="jspResourceBundle" class="com.ibm.lconn.homepage.utils.impl.ResourceBundleImpl" scope="singleton">
        <property name="bundleName" value="com.ibm.lconn.homepage.resources.nls.jsp.jsp_resources"/>
    </bean>
    
    <bean id="catalogServiceResourceBundle" class="com.ibm.lconn.homepage.utils.impl.ResourceBundleImpl" scope="singleton">
        <property name="bundleName" value="com.ibm.lconn.homepage.resources.nls.catalog.service.CatalogServicesLogMessages"/>
    </bean>
        
    <bean id="catalogServiceUIResourceBundle" class="com.ibm.lconn.homepage.utils.impl.ResourceBundleImpl" scope="singleton">
        <property name="bundleName" value="com.ibm.lconn.homepage.resources.nls.catalog.service.ui.CatalogServiceUIMessages"/>
    </bean>
    
    <bean id="mBeanResourceBundle" class="com.ibm.lconn.homepage.utils.impl.ResourceBundleImpl" scope="singleton">
        <property name="bundleName" value="com.ibm.lconn.homepage.resources.nls.mbean.MBeanResources"/>
    </bean>
    
    <bean id="serviceResourceBundle" class="com.ibm.lconn.homepage.utils.impl.ResourceBundleImpl" scope="singleton">
        <property name="bundleName" value="com.ibm.lconn.homepage.resources.nls.service.ServiceLogMessages"/>
    </bean>
    
    <bean id="jfrostResourceBundle" class="com.ibm.lconn.homepage.utils.impl.ResourceBundleImpl" scope="singleton">
        <property name="bundleName" value="com.ibm.lconn.homepage.resources.nls.jfrost.JFrostLogMessages"/>
    </bean>    
    
    <bean id="uiServicesUtils" class="com.ibm.lconn.homepage.utils.impl.UIServicesUtilsImpl" scope="singleton">
        <property name="catalogServiceUIResourceBundle" ref="catalogServiceUIResourceBundle"/>
        <property name="catalogServiceResourceBundle" ref="catalogServiceResourceBundle"/>
        
    </bean>
    
    <bean id="buildStatsResourceBundle" class="com.ibm.lconn.homepage.utils.impl.ResourceBundleImpl" scope="singleton">
        <property name="bundleName" value="com.ibm.lconn.homepage.resources.stats.BuildStats"/>
    </bean>    
    
    <bean id="activityStreamConfigResourceBundle" class="com.ibm.lconn.homepage.utils.impl.ResourceBundleImpl" scope="singleton">
        <property name="bundleName" value="com.ibm.lconn.homepage.resources.nls.activitystream.config.activitystreamconfig_resources"/>
    </bean>  
</beans>
