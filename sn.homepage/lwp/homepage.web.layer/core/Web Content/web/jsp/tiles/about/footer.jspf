<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>
<%@ taglib prefix="corenav" uri="http://www.ibm.com/lconn/tags/corenav" %>
<%@ taglib prefix="coreui"  uri="http://www.ibm.com/lconn/tags/coreuiutil" %>
