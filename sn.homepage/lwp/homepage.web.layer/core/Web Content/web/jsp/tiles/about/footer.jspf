<% if (Environment.SMART_CLOUD.isEnvironment()) { %>
<div id="lotusFooter" class="lotusFooter aboutPage"> <%-- we're adding "aboutPage" to discriminate the about page footer for css reasons --%>
 	<% if ((Boolean)session.getAttribute("isAdmin") == Boolean.TRUE) { %>
		<corenav:footer appname="homepage" admin="true"/>
 	<% } else { %>
		<corenav:footer appname="homepage" admin="false"/>
 	<% } %>
</div>
<% } else { %>
<div id="lotusFooter" class="lotusFooter aboutPage" role="contentinfo">
 	<% if ((Boolean)session.getAttribute("isAdmin") == Boolean.TRUE) { %>
		<corenav:footer appname="homepage" admin="true"/>
 	<% } else { %>
		<corenav:footer appname="homepage" admin="false"/>
 	<% } %>
</div>
<% } %>
