<!-- Package regrouping the actions related to notification tab -->
<struts>
    <package name="widget" extends="homepage-default"> 		
	           
        <action name="displayWidgetPage" class="com.ibm.lconn.homepage.web.action.widget.DisplayWidgetPageAction">
			<interceptor-ref name="widgetPageStack" />		
			<result name="success" type="dispatcher">
                /web/jsp/widgetsTab/widgets.jspf
            </result>
        </action>
    </package>
</struts>

