<beans>
	<bean id="mbeanExporter" class="com.ibm.lconn.homepage.test.services.utils.MockMBeanExporterImpl" init-method="init" destroy-method="destroy">
		<property name="beansToExport">
			<map>
				<entry key="HomepagePersonService" value-ref="homepagePersonService" />
				<entry key="HomepageMetricsService" value-ref="homepageMetricsService" />
				<entry key="HomepageMBeanFacade" value-ref="homepageMBeanFacade" />
			</map>
		</property>
		<property name="wasAdminService" ref="wasAdminService"/>
	</bean>
    
    <bean id="wasAdminService" class="com.ibm.lconn.homepage.test.services.utils.MockWasAdminServiceImpl">
    	<property name="defaultDomain" value="TEST_DEFAULT_DOMAIN"/>
    	<property name="cellName" value="TEST_CELL_NAME"/>
    	<property name="nodeName" value="TEST_NODE_NAME"/>
    	<property name="processName" value="TEST_PROCESS_NAME"/>
    </bean>

</beans>
